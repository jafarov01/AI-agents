# Gemini API Key for AI agents
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: specify different models for different agents
# BIG_MODEL=gemini-1.5-pro-latest
# SMALL_MODEL=gemini-1.5-flash-latest

# GitHub token with repo and workflow scopes
GITHUB_TOKEN=your_github_personal_access_token_here

# Your GitHub username
GITHUB_OWNER=your_github_username

# The name of the repository being worked on (used by orchestrator)
# This can be set dynamically when running the orchestrator
GITHUB_REPO=example-project